                            <key>upsmon[{#UPS.NAME},{#UPS.HOST},battery.charge]</key>
                            <delay>10</delay>
                            <units>%</units>
                            <tags>
                                <tag>
                                    <tag>application</tag>
                                    <value>ups</value>
